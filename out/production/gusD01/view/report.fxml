      <ComboBox fx:id="contactCombo" layoutX="429.0" layoutY="47.0" onAction="#onActionSelectContact" prefWidth="150.0" promptText="Select aTeacher" />
      <TableView fx:id="appointmentLocationTable" layoutX="675.0" layoutY="271.0" prefHeight="235.0" prefWidth="315.0">
         <columns>
            <TableColumn fx:id="LocationD1" prefWidth="161.0" text="Location" />
            <TableColumn fx:id="appointmentsTotalD2" prefWidth="153.0" text="Total Appointments" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
