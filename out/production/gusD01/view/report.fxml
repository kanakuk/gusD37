            <TableColumn fx:id="appointmentId1" prefWidth="36.0" text="ID" />
            <TableColumn fx:id="title2" prefWidth="149.0" text="Title" />
            <TableColumn fx:id="description3" prefWidth="136.0" text="Description" />
            <TableColumn fx:id="type4" prefWidth="57.0" text="Type" />
            <TableColumn fx:id="start5" prefWidth="240.0" text="Start Date / Start Time" />
            <TableColumn fx:id="end6" prefWidth="209.0" text="End Date / End Time" />
            <TableColumn fx:id="customerId7" prefWidth="152.0" text="Parent ID" />
         </columns>
      </TableView>
      <TableView fx:id="appointmentTypeTable" layoutX="19.0" layoutY="271.0" prefHeight="235.0" prefWidth="315.0">
