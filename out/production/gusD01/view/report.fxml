<AnchorPane prefHeight="557.0" prefWidth="1008.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.reportController">
   <children>
      <Label layoutX="480.0" layoutY="14.0" text="Reports">
